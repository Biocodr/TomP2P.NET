<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

  <targets>
    <target xsi:type="Console" name="c"/>
    <!--<target xsi:type="Debugger" name="d"/>-->
  </targets>

  <rules>
    <logger name="TomP2P.Core.*" minlevel="Error" writeTo="c" enabled="true"/>
    <!--<logger name="TomP2P.Core.*" minlevel="Debug" writeTo="c" enabled="false"/>-->
  </rules>
</nlog>